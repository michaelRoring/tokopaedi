</a>
<h2>List Item Tokopakedi</h2>

<table>
  <tr>
    <th>No.</th>
    <th>Item name</th>
    <th>Price</th>
    <th>Stock</th>
    <th>Action</th>
  </tr>
  <% JSON.stringify(items) %>
  <% items.forEach((el, i) => { %>
    <tr>
        <td><%= i+ 1 %></td>
        <td><%= el.name %></td>
        <td><%= el.price %></td>
        <td><%= el.stock %></td>
        <td><a href="/item/<%= el.id %>">View Details</a></td>
    </tr>
  <% }) %>
</table>

</body>
</html>

